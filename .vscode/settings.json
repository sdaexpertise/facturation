// Place your settings in this file to overwrite default and user settings.
{
    "python.pythonPath": "${workspaceRoot}/.venv/bin/python",
    "search.useIgnoreFilesByDefault": true,
    "files.exclude": {
        "**/*.egg-info": true,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/.venv": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "python.linting.enabled": false
}